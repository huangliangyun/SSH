<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>

    <package name="default" extends="struts-default">

        <action name="register_enter" class = "com.javahly.controller.RegisterAction" method ='register_enter'>
            <result name = "register_enter_page">/register.jsp</result>
        </action>

        <action name="register" class = "com.javahly.controller.RegisterAction" method ='execute'>
            <result name = "success">/success.jsp</result>
        </action>

    </package>
</struts>
